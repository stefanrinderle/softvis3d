{
  "name": "softvis3d-frontend",
  "version": "1.0.0",
  "description": "Frontend for the SonarQube-Plugin Softvis3D",
  "main": "src/app.tsx",
  "scripts": {
    "postinstall": "yarn run install:dist && yarn run install:rebuild-sass",
    "install:dist": "shx cp node_modules/react/umd/react.production.min.js node_modules/react-dom/umd/react-dom.production.min.js app/static/",
    "install:rebuild-sass": "node-sass --version || npm rebuild node-sass --no-bin-links",
    "prestart": "yarn run install:rebuild-sass",
    "start": "webpack serve --mode=development",
    "prebuild": "yarn run lint:sources && yarn run test",
    "build": "yarn run build:force",
    "prebuild:ci": "yarn run lint:sources && yarn run test:coverage && yarn run test:sonarqube",
    "build:ci": "yarn run build:force",
    "prebuild:force": "yarn run install:rebuild-sass",
    "build:force": "webpack --progress --mode=production",
    "lint": "yarn run lint:sources && yarn run lint:tests",
    "lint:sources": "eslint --ignore-path .gitignore --ext .ts,.tsx ./src && shx echo \"[eslint] Lint successful (Sources)\"",
    "lint:tests": "eslint --ignore-path .gitignore --ext .ts,.tsx ./test && shx echo \"[eslint] Lint successful (Tests)\"",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:sonarqube": "mocha --reporter mocha-sonarqube-reporter --reporter-options useFullFilePath=true,output=coverage/sonar-coverage.xml",
    "posttest:coverage": "shx rm -r .nyc_output"
  },
  "author": "Stefan Rinderle & Yvo Niedrich",
  "contributors": [
    {
      "name": "Stefan Rinderle",
      "email": "stefan@rinderle.info",
      "url": "https://github.com/stefanrinderle"
    },
    {
      "name": "Yvo Niedrich",
      "email": "yvo.niedrich@gmail.com",
      "url": "https://github.com/Ungolianth"
    }
  ],
  "license": "LGPL-3.0",
  "repository": {
    "type": "http",
    "url": "https://github.com/stefanrinderle/softvis3d.git"
  },
  "dependencies": {
    "@three-ts/orbit-controls": "^1.4.7",
    "@types/chai": "^3.5.2",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/mocha": "^2.2.41",
    "@types/node": "^14.14.6",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.13",
    "@types/sinon": "^2.2.2",
    "@types/three": "^0.127.1",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "codecity-visualizer": "^0.4.6",
    "inversify": "^4.1.1",
    "inversify-inject-decorators": "^3.0.1",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "node-sass": "^4.5.3",
    "react": "^16.14.0",
    "react-custom-scrollbars-2": "^4.4.0",
    "react-dom": "^16.14.0",
    "reflect-metadata": "^0.1.10",
    "sass-loader": "^6.0.5",
    "three": "^0.137.0",
    "three-orbit-controls": "^82.1.0",
    "ts-node": "^3.1.0",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "chai": "^4.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^1.5.1",
    "mocha": "^8.4.0",
    "mocha-sonar-generic-test-coverage": "^0.0.6",
    "mocha-sonarqube-reporter": "^1.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "shx": "^0.2.2",
    "sinon": "^2.3.2",
    "source-map-loader": "^0.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.2.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "mocha": {
    "require": ["jsdom-global/register", "ts-node/register", "reflect-metadata"],
    "reporter": "spec",
    "spec": "test/**/*.spec.ts*",
    "file": "./test/setupEnzyme.ts"
  }
}
